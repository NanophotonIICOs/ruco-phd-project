   "source": [
    "## from math import sin,cos,radians\n",
    "A = 4.00\n",
    "B = 5.00\n",
    "\n",
    "θ1=radians(53.0)\n",
    "θ2=radians(130)\n",
    "\n",
    "Ax = A*cos(θ1) \n",
    "Ay = A*sin(θ1) \n",
    "\n",
    "Bx = B*cos(θ2) \n",
    "By = B*sin(θ2) \n",
    "\n",
    "ApB = Ax*Bx+Ay*By\n",
    "\n",
    "print('A.B=%.2f'%ApB)"
   ]
